      background: #fff;
      border-bottom: 1px solid rgb(229, 233, 241);
      margin-bottom: 30px;
      z-index: 999;
    }

    nav .nav-fix {
      position: inherit;
      border-bottom: 1px solid rgb(229, 233, 241);
    }

    .nav-fix .container {
      position: inherit;
      padding-top: 25px;
      padding-bottom: 28px;
    }

    .navbar-brand {
      position: relative;
      color: rgb(0, 40, 120);
      font-size: 20px;
      font-family: 'Roboto Condensed', sans-serif;
      font-weight: 700;
    }

    .navbar-brand img {
      height: 38px;
    }

    .form-control-fix {
      color: rgb(0, 40, 120);
