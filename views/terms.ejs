      height: 100%;
    }

    .btn-search-fix:focus {
      box-shadow: none;
      border: 0;
    }

    .btn-search-fix i {
      position: absolute;
      top: 48%;
      left: 50%;
      transform: translate(-50%, -50%);
      font-size: 18px;
      color: rgb(0, 40, 120);
    }

    .btn-search-fix:focus i {
      color: #3BB143;
    }

    .btn-panel-fix {
      position: relative;
      width: 30px;
      height: 100%;
      padding: 0;
      margin-left: 12px;
    }

    .btn-panel-fix:focus {
